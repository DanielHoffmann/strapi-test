{
  "name": "nv-cms",
  "private": true,
  "version": "0.1.0",
  "description": "A Strapi application",
  "scripts": {
    "api-watch": "concurrently \"tsc -w\" \"node copyApiJsons.js --watch\"",
    "develop": "concurrently \"yarn run api-watch\" \"nodemon --ignore api_ts/ server.js\"",
    "debug": "concurrently \"yarn run api-watch\" \"nodemon --inspect=0.0.0.0:9229 --ignore api_ts/ server.js\"",
    "start": "strapi start",
    "codegen": "sts api_ts/ -g components/ -o api_ts/types",
    "type-check": "tsc --noEmit",
    "build": "yarn run codegen && tsc && node copyApiJsons.js && strapi build",
    "postgres": "docker run --name nv-cms -e POSTGRES_PASSWORD=postgres -p 5432:5432 -d postgres"
  },
  "devDependencies": {
    "@types/node": "^14.14.9",
    "@types/qrcode": "^1.3.5",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6",
    "strapi-to-typescript": "^2.0.1",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "knex": "<0.20.0",
    "pg": "latest",
    "qrcode": "^1.4.4",
    "strapi": "3.3.3",
    "strapi-admin": "3.3.3",
    "strapi-connector-bookshelf": "3.3.3",
    "strapi-plugin-content-manager": "3.3.3",
    "strapi-plugin-content-type-builder": "3.3.3",
    "strapi-plugin-email": "3.3.3",
    "strapi-plugin-upload": "3.3.3",
    "strapi-plugin-users-permissions": "3.3.3",
    "strapi-utils": "3.3.3"
  },
  "author": {
    "name": "A Strapi developer"
  },
  "strapi": {
    "uuid": "d7f09353-38c1-4064-83ca-9f098df874b9"
  },
  "engines": {
    "node": ">=10.16.0 <=14.x.x",
    "npm": ">=6.0.0"
  },
  "license": "MIT"
}
